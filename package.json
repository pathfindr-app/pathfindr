{
  "name": "pathfindr",
  "version": "1.0.0",
  "description": "Race against AI to find the shortest path through real city streets",
  "main": "game.js",
  "scripts": {
    "start": "npx serve .",
    "build": "node scripts/build.js",
    "build:mobile": "npm run build && npx cap sync",
    "ios": "npm run build:mobile && npx cap open ios",
    "android": "npm run build:mobile && npx cap open android",
    "add:ios": "npx cap add ios",
    "add:android": "npx cap add android"
  },
  "keywords": [
    "game",
    "pathfinding",
    "maps",
    "astar"
  ],
  "author": "Pathfindr",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@capacitor-community/admob": "^7.2.0",
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@revenuecat/purchases-capacitor": "^12.0.3",
    "@supabase/supabase-js": "^2.89.0"
  }
}
